{
  "ApplicationName":  "GrpcTraining",

  "OpenDotaConfig": {
    "ApiKey": "SecretApiKey",
    "BaseApiUrl": "https://api.opendota.com/api"
  },
  "ElasticSearchConfig": {
    "Uri": "http://localhost:9200"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
